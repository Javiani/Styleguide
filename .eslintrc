{
    "rules": {
        "indent": [2,"tab"],
        "quotes": [2,"single"],
        "linebreak-style": [2,"unix"],
        "semi": [2,"never"],
        "no-unused-vars": ["warn", { "vars": "all", "args": "none" }],
        "no-console": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "node" : true
    },
    "extends": "eslint:recommended",
     "parserOptions": {
        "sourceType": "module"
    }
}
